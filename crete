    <script>
        // Function to fetch events from local storage and display them in the table
        function fetchEvents() {
            const events = JSON.parse(localStorage.getItem('events')) || [];
            const eventsContainer = document.getElementById('eventsContainer');
            eventsContainer.innerHTML = ''; // Clear previous events
 
            events.forEach((event, index) => {
                const status = event.approved ? 'Approved' : 'Pending'; // Assuming there is an approved flag
                const eventRow = `
                    <tr>
                        <td>${event.title}</td>
                        <td>${event.type}</td>
                        <td>${new Date(event.startDate).toLocaleString()}</td>
                        <td>${new Date(event.endDate).toLocaleString()}</td>
                        <td>${status}</td>
                        <td>
                            <button class="btn btn-info" onclick="viewEvent(${index})">View</button>
                            <button class="btn btn-warning" onclick="editEvent(${index})">Edit</button>
                            <button class="btn btn-danger" onclick="confirmDeleteEvent(${index})">Delete</button>
                        </td>
                    </tr>
                `;
                eventsContainer.insertAdjacentHTML('beforeend', eventRow);
            });
        }
 
        // Call the function to fetch events on page load
        document.addEventListener('DOMContentLoaded', fetchEvents);
    </script>

    <!-- ..::  scripts  end ::.. -->

